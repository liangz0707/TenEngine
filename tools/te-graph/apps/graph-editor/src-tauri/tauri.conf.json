{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "TE Graph Editor",
  "version": "0.1.0",
  "identifier": "com.tenengine.graph.editor",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://127.0.0.1:5174",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "TE Graph Editor",
        "width": 1400,
        "height": 900,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false
  }
}
